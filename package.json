{
	"name": "vitrine-admin-cli",
	"version": "0.0.1",
	"license": "MIT",
	"bin": "dist/cli.js",
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "tsc",
		"start": "npm run build && chmod +x dist/cli.js && dist/cli.js",
		"pretest": "npm run build",
		"test": "xo && ava",
		"postinstall": "tsc"
	},
	"files": [
		"dist/cli.js"
	],
	"dependencies": {
		"ink": "^3.2.0",
		"ink-link": "^2.0.0",
		"ink-spinner": "^4.0.3",
		"ink-text-input": "^4.0.2",
		"isomorphic-fetch": "^3.0.0",
		"meow": "^9.0.0",
		"minimatch": "^3.0.4",
		"open": "^8.4.0",
		"react": "^17.0.2"
	},
	"ava": {
		"typescript": {
			"extensions": [
				"tsx"
			],
			"rewritePaths": {
				"source/": "dist/"
			}
		}
	},
	"xo": {
		"extends": "xo-react",
		"rules": {
			"react/prop-types": "off"
		}
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.0",
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/isomorphic-fetch": "^0.0.35",
		"@types/minimatch": "^3.0.5",
		"@types/open": "^6.2.1",
		"@types/react": "^17.0.35",
		"ava": "^3.15.0",
		"chalk": "^4.1.2",
		"eslint-config-xo-react": "^0.25.0",
		"eslint-plugin-react": "^7.27.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"ink-testing-library": "^2.1.0",
		"typescript": "^4.4.4",
		"xo": "^0.39.1"
	}
}
